   ],
   "source": [
    "from sympy.abc import x, y\n",
    "from sympy import diff, pprint\n",
    "\n",
    "\n",
    "f = x**2 + 2 * y**2\n",
    "dx = diff(f, x)\n",
    "dy = diff(f, y)\n",
    "\n",
    "print(\"Derivative of\")\n",
    "pprint(f)\n",
    "print(\"with respect to x is\")\n",
    "pprint(dx)\n",
    "print(\"and with respect to y is\")\n",
    "pprint(dy)"
   ]
