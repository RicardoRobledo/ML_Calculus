   "source": [
    "import numpy as np\n",
    "\n",
    "def g(x):\n",
    "    return 1-x\n",
    "\n",
    "epsilon = np.finfo(float).eps\n",
    "print(\"Left limit is\", g(x-epsilon))\n",
    "print(\"Right limit is\", g(x+epsilon))"
   ]
